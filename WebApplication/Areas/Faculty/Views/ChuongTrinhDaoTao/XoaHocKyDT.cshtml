@model WebApplication.Models.HocKyDaoTao

@{
    ViewBag.Title = "DeleteKhoa";
    Layout = "~/Views/Shared/Faculty_Layout.cshtml";
}

<h2>
    Xóa Học kỳ @Model.HocKy
</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Học kỳ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HocKy)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa học kỳ" class="btn btn-secondary btn-rounded" /> 
            <input type="submit" value="Trở về" class="btn btn-secondary btn-rounded" formaction="@Url.Action("ListKhoaDT")" />
        </div>
    }
</div>
